      "ONAM": "Tienius Delitian",
      "RNAM": null,
      "CNAM": null,
      "FNAM": null,
      "ANAM": null,
      "DNAM": null,
      "SNAM": null,
      "NAME": "Good. Report to me when you've found this anonymous writer and made sure that he will never print lies about King Helseth again.",
      "BNAM": "Journal TR09_Journalist 1",
      "SCVR": [
        {
          "index": "0",
          "scvr_type": "1",
          "details": "50",
          "operator": "0",
